{
    "name": "phwoolcon/demo",
    "description": "Demo module for Phwoolcon",
    "keywords": ["phwoolcon", "demo", "phalcon", "swoole"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Christopher CHEN",
            "email": "fishdrowned@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "phwoolcon/phwoolcon": "~1.0",
        "phwoolcon/auth": "dev-master",
        "phwoolcon/payment": "~1.0"
    },
    "require-dev": {
        "techpivot/phalcon-ci-installer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phwoolcon\\Demo\\": "src",
            "Phwoolcon\\Demo\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
